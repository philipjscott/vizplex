{
  "name": "vizplex",
  "descript": "GLSL noise visualizer library",
  "version": "1.0.4",
  "main": "./dist/vizplex.min.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.lib.config.js --env build",
    "lib": "webpack --config webpack.lib.config.js",
    "demo": "webpack --config webpack.demo.config.js",
    "dev": "webpack-dev-server --config webpack.demo.config.js --open",
    "test": "mocha --watch --require reify test",
    "docs": "markdown-preview README.md"
  },
  "keywords": [
    "simplex",
    "noise",
    "glsl",
    "visualizer",
    "visualizations",
    "generative",
    "generative-art",
    "art",
    "procedural",
    "animated"
  ],
  "dependencies": {
    "a-big-triangle": "^1.0.3",
    "canvas-fit": "^1.5.0",
    "ccapture.js": "^1.0.7",
    "gl-clear": "^2.0.0",
    "gl-context": "^0.1.1",
    "gl-reset": "^1.0.0",
    "gl-shader": "^4.2.1",
    "glsl-noise": "^0.0.0",
    "glslify": "^6.1.0",
    "right-now": "^1.0.0"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "buble-loader": "^0.4.1",
    "glslify-loader": "^2.0.0",
    "mocha": "^6.1.4",
    "mocha-loader": "^2.0.1",
    "raw-loader": "^0.5.1",
    "reify": "^0.12.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^3.1.11"
  },
  "repository": "https://github.com/ScottyFillups/vizplex.git",
  "author": "Philip Scott <pscott@zeptohost.com>"
}
